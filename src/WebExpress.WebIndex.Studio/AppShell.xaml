<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="WebExpress.WebIndex.Studio.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:pages="clr-namespace:WebExpress.WebIndex.Studio.Pages"
    Shell.FlyoutBehavior="Flyout"
    Title="WebExpress.WebIndex.Studio">

    <FlyoutItem Title="Project">
        <Tab>
            <ShellContent Title="WebIndex.Studio - Project" ContentTemplate="{DataTemplate pages:ProjectPage}" />
        </Tab>
    </FlyoutItem>
    <FlyoutItem Title="Info" FlyoutIcon="?">
        <Tab>
            <ShellContent Title="WebIndex.Studio - Info" ContentTemplate="{DataTemplate pages:InfoPage}" />
        </Tab>
    </FlyoutItem>

</Shell>
